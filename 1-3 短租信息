from bs4 import BeautifulSoup
import requests
url = 'http://cq.xiaozhu.com/fangzi/4783366714.html'
wb_data = requests.get(url)
soup = BeautifulSoup(wb_data.text,"lxml")
titles = soup.select('body > div.wrap.clearfix.con_bg > div.con_l > div.pho_info > h4 > em')
adds = soup.select('body > div.wrap.clearfix.con_bg > div.con_l > div.pho_info > p > span')
prices = soup.select('#pricePart > div.day_l > span')
imgs = soup.select('#imgMouseCusor')
holders = soup.select('#floatRightBox > div.js_box.clearfix > div.member_pic > a > img')
sexuality = soup.select('#floatRightBox > div.js_box.clearfix > div.w_240 > h6 > span')
print(titles,adds,prices,imgs,holders,sexuality)
